               "legendFormat": "{{acmetel_stage_kind}} - {{acmetel_stage_name}}"
            }
         ],
         "title": "Active Workers",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "gridPos": {
            "h": 8,
            "w": 24,
            "x": 0,
            "y": 26
         },
         "id": 9,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "tempo",
                  "uid": "$traces"
               },
               "filters": [ ],
               "limit": 20,
               "query": "{resource.service.name=\"sc-test-telemetry\" && traceDuration>100ms}",
               "spss": 10
            }
         ],
         "title": "Traces",
         "type": "table"
      }
   ],
   "refresh": "1s",
   "schemaVersion": 39,
   "templating": {
      "list": [
         {
            "name": "metrics",
            "query": "prometheus",
            "type": "datasource"
         },
         {
            "name": "traces",
            "query": "tempo",
            "type": "datasource"
         }
      ]
   },
   "time": {
      "from": "now-15m",
