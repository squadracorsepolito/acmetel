{
   "graphTooltip": 1,
   "panels": [
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "fieldConfig": {
            "defaults": {
               "color": {
                  "fixedColor": "green",
                  "mode": "fixed"
               },
               "unit": "Bps"
            }
         },
         "gridPos": {
            "h": 6,
            "w": 6,
            "x": 0,
            "y": 0
         },
         "id": 1,
         "options": {
            "colorMode": "value",
            "graphMode": "area",
            "showPercentChange": false
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$metrics"
               },
               "expr": "rate(received_bytes_total[$__rate_interval])",
               "interval": "15s"
            }
         ],
         "title": "Received Bytes Rate",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
