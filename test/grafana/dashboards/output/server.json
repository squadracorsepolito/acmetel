         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$metrics"
               },
               "expr": "delivered_rows_total",
               "interval": "15s"
            }
         ],
         "title": "Inserted Rows",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "fieldConfig": {
            "defaults": {
               "color": {
                  "mode": "palette-classic"
               },
               "custom": {
                  "fillOpacity": 50,
                  "lineInterpolation": "smooth",
                  "lineWidth": 2
               },
               "unit": "ms"
            }
         },
         "gridPos": {
            "h": 18,
            "w": 12,
            "x": 0,
            "y": 18
         },
         "id": 7,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$metrics"
               },
               "expr": "histogram_quantile(0.95, sum by(le) (rate(total_message_processing_time_milliseconds_bucket[$__rate_interval])))",
               "interval": "15s",
               "legendFormat": "p95"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$metrics"
               },
               "expr": "histogram_quantile(0.90, sum by(le) (rate(total_message_processing_time_milliseconds_bucket[$__rate_interval])))",
               "interval": "15s",
               "legendFormat": "p90"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$metrics"
               },
               "expr": "histogram_quantile(0.75, sum by(le) (rate(total_message_processing_time_milliseconds_bucket[$__rate_interval])))",
               "interval": "15s",
               "legendFormat": "p75"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$metrics"
               },
               "expr": "histogram_quantile(0.50, sum by(le) (rate(total_message_processing_time_milliseconds_bucket[$__rate_interval])))",
               "interval": "15s",
               "legendFormat": "p50"
            }
         ],
         "title": "Message Processing Time",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "fieldConfig": {
            "defaults": {
               "color": {
                  "mode": "palette-classic"
               },
               "custom": {
                  "fillOpacity": 0,
                  "lineInterpolation": "stepAfter",
                  "lineWidth": 2
               }
            }
         },
         "gridPos": {
            "h": 18,
            "w": 12,
            "x": 12,
            "y": 18
         },
         "id": 8,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$metrics"
